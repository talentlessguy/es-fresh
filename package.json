{
  "name": "es-fresh",
  "description": "fresh rewrite in TypeScript with ESM and CommonJS targets",
  "version": "0.0.3",
  "repository": "https://github.com/talentlessguy/es-fresh.git",
  "engines": {
    "node": ">=13"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "talentlessguy <pilll.PL22@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
